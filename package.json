{
  "name": "task3-webdriverio-telnyx",
  "type": "module",
  "devDependencies": {
    "@wdio/allure-reporter": "^9.4.2",
    "@wdio/cli": "^9.4.1",
    "@wdio/local-runner": "^9.4.1",
    "@wdio/mocha-framework": "^9.2.8",
    "@wdio/spec-reporter": "^9.2.14",
    "cross-env": "^7.0.3"
  },
  "scripts": {
    "test:all:headless": "wdio run wdio.conf.js --headless",
